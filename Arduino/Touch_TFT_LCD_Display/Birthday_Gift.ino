uint8_t PORTRAIT = 0;    //PORTRAIT
uint8_t LANDSCAPE = 1;   // Landscape
uint8_t currentState;
uint8_t Direction;

TouchScreen ts = TouchScreen(XP, YP, XM, YM, 300);
TSPoint tp;

#define MINPRESSURE 200
#define MAXPRESSURE 1000

#define BACK 0
#define NEXT 1

// Assign human-readable names to some common 16-bit color values:
#define	BLACK   0x0000
#define WHITE   0xFFFF
#define HOTPINK 0xECF8


void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
  uint32_t when = millis();
  //    while (!Serial) ;             //hangs a Leonardo until you connect a Serial
  if (!Serial) delay(1000);           //allow some time for Leonardo
  Serial.println("Serial took " + String((millis() - when)) + "ms to start");

  ///////////////////////////////////////////////////////////////////////////
  delay(1000); //Add this delay for final code
  tft.reset();                                    //hardware reset
  uint16_t ID = tft.readID(); 
  Serial.print("ID = 0x");
  Serial.println(ID, HEX);
  if (ID == 0xD3D3) ID = 0x9481;                 // write-only shield
  //    ID = 0x9329;                             // force ID
  tft.begin(ID);
  ///////////////////////////////////////////////////////////////////////////
  
  transitionAnimation();
  delay(1000);
  drawHomeScreen();
  /* Test Code
        // tft.fillScreen(HOTPINK);
        tft.drawLine(0,250,240,250, WHITE);
  */
  //////////////////////////////////////////////////////////////////////////
  currentState = 0;
}

void loop() {
  // Getting TouchScreen Input to determine 
  uint8_t Orientation = PORTRAIT;
  uint16_t xpos, ypos;  //screen coordinates
  tp = ts.getPoint();   //tp.x, tp.y are ADC values

  // if sharing pins, you'll need to fix the directions of the touchscreen pins
  pinMode(XM, OUTPUT);
  pinMode(YP, OUTPUT);
  // we have some minimum pressure we consider 'valid'
  // pressure of 0 means no pressing!

  if (tp.z > MINPRESSURE && tp.z < MAXPRESSURE) {
    // most mcufriend have touch (with icons) that extends below the TFT
    // screens without icons need to reserve a space for "erase"
    // scale the ADC values from ts.getPoint() to screen values e.g. 0-239
    //
    // Calibration is true for PORTRAIT. tp.y is always long dimension 
    // map to your current pixel orientation
      
    switch (Orientation) {
        case 0:
            xpos = map(tp.x, 930, 180, 0, 240);
            ypos = map(tp.y, 169, 919, 0, 320);
            break;
        case 1:
            xpos = map(tp.y, 169, 919, 0, 320);
            ypos = map(tp.x, 180, 930, 0, 240);
            break;
    }
    // Serial.print("xpos = ");
    // Serial.println(xpos, DEC);

    if (ypos>245){
      if (xpos<120){
        Direction = BACK;        
      }
      else {
        Direction = NEXT;
      }
      stateChange(Direction);
    }
  } 
}


void stateChange(int direction) {
  if (direction){
    currentState++;
  }
  else {
    currentState--;    
  }
  switch (currentState) {
    case 1:
      initSlideShow();
      break;
    case 2:
      drawPageOne();
      break;
    case 3:
      drawPageTwo();
      break;
    default: currentState=0;
  }
}

